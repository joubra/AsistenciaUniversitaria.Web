@model AsistenciaUniversitaria.Web.Models.Recinto

@{
    ViewData["Title"] = "Eliminar Recinto";
}

<h1>Eliminar Recinto</h1>

<div>
    <h4>¿Está seguro que quiere eliminar este recinto?</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Nombre</dt>
        <dd class="col-sm-10">@Model.Nombre</dd>
        <dt class="col-sm-2">Estado</dt>
        <dd class="col-sm-10">@Model.Estado</dd>
    </dl>

    <form asp-action="DeleteConfirmed" method="post">
        <input type="hidden" asp-for="Id" />
        <button type="submit" class="btn btn-danger">Eliminar</button>
        <a asp-action="Index" class="btn btn-secondary">Cancelar</a>
    </form>
</div>
